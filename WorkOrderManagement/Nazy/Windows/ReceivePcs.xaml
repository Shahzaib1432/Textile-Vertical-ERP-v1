<Window x:Class="WorkOrderManagement.Nazy.Windows.ReceivePcs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkOrderManagement.Nazy.Windows"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        WindowStartupLocation="CenterScreen"
        Title="Receive Pcs:" Height="700" Width="900">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <ScrollViewer>
            <StackPanel x:Name="RowsCont" Orientation="Vertical" Margin="10"/>
        </ScrollViewer>
        <Button Grid.Row="1" Content="SUBMIT" Foreground="DarkGray" Background="WhiteSmoke" FontWeight="ExtraBold" 
                FontSize="15" Padding="0 10 0 10" Margin="10" Click="Submit_Click" BorderBrush="LightGray"/>
    </Grid>
</Window>
