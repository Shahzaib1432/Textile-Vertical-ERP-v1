<Window x:Class="EMBOrderManagement.Windows.OrderDemand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMBOrderManagement.Windows"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="OrderDemand" Height="650" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <StackPanel x:Name="RowsContainer" Margin="10"/>
        <Button Name="SubmitBtn" Grid.Row="1" Content="SUBMIT" FontWeight="ExtraBold" Foreground="Gray" Background="#E9E9E9" FontSize="20" Padding="10" Margin="10"/>
    </Grid>
</Window>
